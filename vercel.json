{
    "version": 2,
    "rewrites": [
        { "source": "/(.*)", "destination": "/api" }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        } ,
        {
            "source": "/inventory-management-mgl5.vercel.app/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
          } 
    ]
}